package pack1;

public class ObjClas {

   
	   
		String s ;
		int i ;
		double d ;
		boolean b  ;
	

	public static void main(String[] args) {
		
		
		
	ObjClas oc = new ObjClas();
	String s1 = oc.s = "Hello";
	    int i1= oc.i = 2023;
	 double d1= oc.d = 04.17;
	boolean b1 = oc.b = true ;
		
	System.out.println(s1);
	System.out.println(i1);
	System.out.println(d1);Hello
